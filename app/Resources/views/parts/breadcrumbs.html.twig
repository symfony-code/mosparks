<ol class="breadcrumb">

    {% if breadcrumbs %}
        <li><a href="{{ path('homepage') }}">Администрирование</a></li>
        {% for breadcrumb in breadcrumbs %}
            {% if breadcrumb.url is defined %}
                <li><a href="{{ breadcrumb.url }}">{{ breadcrumb.label }}</a></li>
            {% else %}
                <li class="active">{{ breadcrumb.label }}</li>
            {% endif %}
        {% endfor %}
    {% endif %}
</ol>